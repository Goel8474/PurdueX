public class Robot {
  void speak() {
    System.out.println("hi!");
  }
  
  void speak2() {
    System.out.println("bye!");
  }
  public static void main(String[] args) {
    Robot r = new Robot();
    r.speak();
    Robot r = new Robot();
    r.speak2();
  }
}